    "npm": ">=5.0.0"
  },
  "typings": "typings/ractive.d.ts",
  "devDependencies": {
    "@reactivex/rxjs": "^5.5.1",
    "acorn": "^5.3.0",
    "ampersand-collection": "^2.0.1",
    "ampersand-model": "^8.0.1",
    "backbone": "^1.3.3",
    "baconjs": "^1.0.1",
    "browserify": "^15.2.0",
    "commander": "^2.13.0",
    "fs-plus": "^3.0.1",
    "jquery": "^3.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-qunit": "^1.2.1",
    "karma-tape-reporter": "^1.0.3",
    "magic-string": "^0.22.4",
    "puppeteer": "^1.0.0",
    "qunit": "^2.4.1",
    "qunit-assert-html": "^1.1.0",
    "qunitjs": "^2.4.0",
    "rollup": "^0.50.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "serialize-javascript": "^1.4.0",
    "simulant": "^0.2.2",
    "standard": "^10.0.3",
    "tippex": "^3.0.0",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "wipe": "find ./packages/node_modules/@ractivejs/ -type d \\( -name dist -o -name tmp -o -name bin \\) -exec rm -rf {} +",
    "exec": "cd ./packages/node_modules/@ractivejs/$npm_config_with && npm run $npm_config_do",
    "make": "< .build xargs -I % npm run exec --do='make' --with=\"%\"",
    "test": "< .build xargs -I % npm run exec --do='test' --with=\"%\"",
    "both": "npm run exec --do='make' --with=\"$npm_config_with\" && npm run exec --do='test' --with=\"$npm_config_with\""
  }
}
